
<!DOCTYPE html>

<html lang="fa">

<partial name="_AdminHead"/>

@await RenderSectionAsync("Css",false)

<body class="hold-transition skin-blue sidebar-mini">
<div class="wrapper">
	


	<partial name="_AdminMainHeader"/>
	<partial name="_AdminSideBar"/>
	

	<div class="content-wrapper">
		
		<h2 class="display-2 " style="margin-right: 10px">@ViewData["Title"]</h2>

		@await RenderSectionAsync("HeaderContent", false)


		@RenderBody()
	</div>

	<partial name="_AdminFooter"/>

</div>

	

<partial name="_AdminScripts"/>
 
@await RenderSectionAsync("Scripts",false)
</body>

</html>