@using Shared.Application.Utility
@model Site.Application.Contract.SiteSettingApplication.Query.LogoForUiQueryModel


@if (!string.IsNullOrEmpty(Model.LogoName))
{
	<a href="/blog" class="logo">
		<img class="logo__img" src="@Directories.GetLogoImageFullPath(Model.LogoName,100) " @(string.IsNullOrEmpty(Model.LogoAlt) ? "" : $"alt={Model.LogoAlt}") width="60px"/>
	</a>
}
