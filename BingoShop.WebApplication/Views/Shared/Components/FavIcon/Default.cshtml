@using Shared.Application.Utility
@model Site.Application.Contract.SiteSettingApplication.Query.FavIconForUiQueryModel

@if(!string.IsNullOrEmpty(Model.FavIcon))
{
	<link rel="icon" type="image/png" href="@Directories.GetFavIconImageFullPath(Model.FavIcon,32)">
	<link rel="apple-touch-icon" href="@Directories.GetFavIconImageFullPath(Model.FavIcon,32)">
}